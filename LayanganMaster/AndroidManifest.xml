<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="flyingrabbit.layanganmaster"
    android:versionCode="81"
    android:versionName="1.4.6" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />
    <uses-feature
        android:name="android.hardware.touchscreen"
        android:required="false" />
    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />
    <uses-feature
        android:name="android.hardware.camera.autofocus"
        android:required="false" />

    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="flyingrabbit.layanganmaster.game.Test"
            android:label="@string/title_activity_test" >
        </activity>
        <activity
            android:name="flyingrabbit.layanganmaster.game.EndStory"
            android:label="@string/title_activity_end_story" >
        </activity>
        <activity
            android:name="flyingrabbit.layanganmaster.game.SplashScreen"
            android:label="@string/title_activity_splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="flyingrabbit.layanganmaster.game.Menu"
            android:label="@string/title_activity_menu" >
        </activity>
        <activity
            android:name="flyingrabbit.layanganmaster.game.Lari"
            android:label="@string/title_activity_lari" >
        </activity>
        <activity
            android:name="flyingrabbit.layanganmaster.game.Compass"
            android:label="@string/title_activity_compass" >
        </activity>
        <activity
            android:name="flyingrabbit.layanganmaster.game.NonAugmentedActivity"
            android:label="@string/title_activity_non_augmented" >
        </activity>
        <activity
            android:name="flyingrabbit.layanganmaster.game.StartStory"
            android:label="@string/title_activity_start_story" >
        </activity>
        <activity
            android:name="flyingrabbit.layanganmaster.game.AugmentedActivity"
            android:label="@string/title_activity_augmented" >
        </activity>
    </application>

</manifest>